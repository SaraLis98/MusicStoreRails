<%= form_with(model: @instrumento, local: true) do |form| %>
  <div>
    <%= form.label :nombre %>
    <%= form.text_field :nombre %>
  </div>

  <div>
    <%= form.label :tipo %>
    <%= form.text_field :tipo %>
  </div>

  <div>
    <%= form.label :precio %>
    <%= form.text_field :precio %>
  </div>

  <div>
    <%= form.label :detalles %>
    <%= form.text_field :detalles %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>